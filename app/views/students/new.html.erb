<h3>Student Form</h3>

<%=form_tag students_path do%>
<label>First Name</label><br>
<%= text_field_tag :'student[first_name]', value ='Margaery'%>

<label>Last Name</label><br>
<%= text_field_tag :'student[last_name]', value ='params[:student][:last_name]'%>
<%= submit_tag "Submit Student"%>
<%end%>
